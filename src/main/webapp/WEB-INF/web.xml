<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">

  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>TransferController</servlet-name>
  	<servlet-class>com.bank.controller.TransferController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TransactionRestController</servlet-name>
  	<servlet-class>com.bank.controller.TransactionRestController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>TransferController</servlet-name>
  	<url-pattern>/TransferController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TransactionRestController</servlet-name>
  	<url-pattern>/TransactionRestController</url-pattern>
  </servlet-mapping>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
</web-app>
